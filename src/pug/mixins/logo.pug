mixin logo(currentPage)
	if (currentPage === "index.html")
		a.logo(class!=attributes.class)
			img(src="images/nerds-logo.svg" width="140" height="49" alt="Логотип Nerds")
	else
		a.logo(class!=attributes.class href="index.html")
			img(src="images/nerds-logo.svg" width="140" height="49" alt="Логотип Nerds")
